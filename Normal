#include <stdio.h>
#include <stdlib.h>
void organiza (int v[],int N,trocador,int N=10)
{
    int IMPLEMENTA,IMPLEMENTA2,trocador;
    for (IMPLEMENTA=1;IMPLEMENTA<N;IMPLEMENTA++)
{
    for(IMPLEMENTA2=1;IMPLEMENTA2<N-1;IMPLEMENTA2++)
    {
        if (int vetor[IMPLEMENTA2]>int[IMPLEMENTA2+1]){
            trocador=vetor[IMPLEMENTA2]
            vetor[IMPLEMENTA2]=vetor[IMPLEMENTA2+1]
            trocador=vetor[IMPLEMENTA2+1]
            }
    }
}
}
int main() {
    int v[]={1,3,4,5,6,7,8,9,2}, i,N=10;
    organiza( v, N);
    for (i=1;i<N;i++){
            printf("%d",v[i]);
    }
}
